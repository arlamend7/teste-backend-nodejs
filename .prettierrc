{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 4,
  "trailingComma": "none",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "overrides": [
      {
          "files": ["*.ts"],
          "options": {
              "printWidth": 90
          }
      },
      {
        "files": ["*.spec.ts"],
        "options": {
            "printWidth": 120
        }
    }
  ]
}